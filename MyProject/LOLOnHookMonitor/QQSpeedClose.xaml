<Window x:Class="LOLOnHookMonitor.QQSpeedClose"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LOLOnHookMonitor"
        mc:Ignorable="d"
        Title="QQSpeedClose" Height="175" Width="255">
    <Grid >
        <StackPanel Orientation="Horizontal" Height="20" VerticalAlignment="Top" >
            <TextBox Text="00"/>
            <TextBlock Text="时"/>
            <TextBox Text="01"/>
            <TextBlock Text="分"/>

        </StackPanel>
        <Button Content="定时关闭飞车" Margin="10,25,100,75" Click="Button_Click" />
    </Grid>
</Window>
