<Window x:Class="Quickspot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quickspot"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="200">
    <StackPanel>
        <Button Width="150" Height="50" Content="开始" Click="Compare_Click" Margin="0 20 0 0"/>
        <Button Width="150" Height="50" Content="重置" Click="Clear_Click" Margin="0 20 0 0"/>
    </StackPanel>
</Window>
