<Window x:Class="GTA挂机.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GTA挂机"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="250">
    <StackPanel>
        <Button x:Name="btn" Content="开始" Width="120" Height="50" Click="Button_Click"/>
        <TextBox x:Name="txtbox" Text=""/>
        <Button x:Name="btn2" Content="开始2" Width="120" Height="50" Click="Button2_Click" Visibility="Visible"/>
        <Button x:Name="btn3" Content="开始3" Width="120" Height="50" Click="btn3_Click" Visibility="Collapsed"/>
        <Button x:Name="btn4" Content="显示隐藏" Width="120" Height="50" Click="btn4_Click" Visibility="Visible"/>
    </StackPanel>
</Window>
